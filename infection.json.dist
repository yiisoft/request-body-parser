{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php:\/\/stderr",
        "stryker": {
            "report": "master"
        }
    },
    "mutators": {
        "@default": true,
        "IncrementInteger": {
            "ignoreSourceCodeByRegex": [
                ".* explode\\(';', \\$contentType, 2\\);$"
            ]
        }
    }
}
